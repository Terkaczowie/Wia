		<script type="text/javascript">
			function suma(){
			var x=Number(document.getElementById("id1").value); 
			var y=Number(document.getElementById("id2").value);
			var z=x+y;
			document.getElementById("wynik").innerHTML=x+" + "+y+" = "+z;
			}
			function roznica(){
			var x=Number(document.getElementById("id1").value); 
			var y=Number(document.getElementById("id2").value);
			var z=x-y
			document.getElementById("wynik").innerHTML=x+" - "+y+" = "+z;
			}
			function iloczyn(){
			var x=Number(document.getElementById("id1").value); 
			var y=Number(document.getElementById("id2").value);
			var z=x*y
			document.getElementById("wynik").innerHTML=x+" * "+y+" ="+z;
			}
			function iloraz(){
			var x=Number(document.getElementById("id1").value); 
			var y=Number(document.getElementById("id2").value);
			var z=x/y
			document.getElementById("wynik").innerHTML=x+" / "+y+" = "+z;
			}


		</script>
		<br>
		Kalkulator<br><br>
		Podaj pierwszą liczbę: <input type="Number" id="id1"/>
		<br>
		<br>
		Podaj drugą liczbę: <input type="Number" id="id2"/>
		<br>
		<br>
		<input type="button" onClick="suma()" value="+"/>
		<input type="button" onClick="roznica()" value="-"/>
		<input type="button" onClick="iloczyn()" value="*"/>
		<input type="button" onClick="iloraz()" value="/"/>
		<div id=wynik> </div>
